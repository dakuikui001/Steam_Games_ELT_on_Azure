		"cells": [
			{
				"cell_type": "code",
				"source": [
					"# 1. 先安装关键的基础依赖\n",
					"!pip install \"typing_extensions>=4.10.0\"\n",
					"\n",
					"# 2. 安装 Great Expectations 及其它依赖\n",
					"!pip install \"great_expectations==1.10.0\"\n",
					"\n",
					"# 3. 修复你之前日志里提到的 sympy 冲突（torch 需要 1.13.1）\n",
					"!pip install \"sympy==1.13.1\""
				],
				"execution_count": 1
			},
			{
				"cell_type": "code",
				"source": [
					"%run \"great_expectations_processing\""
				],
				"execution_count": 2
			},
			{
				"cell_type": "code",
				"metadata": {
					"tags": [
						"parameters"
					]
				},
				"source": [
					"Once = True\n",
					"ProcessingTime = '5 seconds'"
				],
				"execution_count": 3
			},
			{
				"cell_type": "code",
				"source": [
					"spark.sql(\"USE steamdb\")"
				],
				"execution_count": 4
			},
			{
				"cell_type": "code",
				"source": [
					"%run \"02_bronze\""
				],
				"execution_count": 5
			},
			{
				"cell_type": "code",
				"source": [
					"bronze = Bronze()\n",
					"bronze.consume(Once, ProcessingTime)"
				],
				"execution_count": 6
			},
			{
				"cell_type": "code",
				"source": [
					"%run \"03_silver\""
				],
				"execution_count": 7
			},
			{
				"cell_type": "code",
				"source": [
					"silver = Silver()\n",
					"silver.upsert(Once, ProcessingTime)"
				],
				"execution_count": 8
			},
			{
				"cell_type": "code",
				"source": [
					"%run \"04_gold\""
				],
				"execution_count": 9
			},
			{
				"cell_type": "code",
				"source": [
					"gold = Gold()\n",
					"gold.upsert(Once, ProcessingTime)"
				],
				"execution_count": 10
			}
		]
	}
